  "author": "rlnsy",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlnsy/pocket-api-lib"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "zod": "^3.17.3"
  }
