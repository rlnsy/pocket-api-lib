{
  "name": "pocket-api-lib",
  "version": "2022.6.30",
  "description": "TypeScript client library for Pocket API (https://getpocket.com/developer)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "ts-node e2e_test.ts",
